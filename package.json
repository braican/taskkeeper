{
  "name": "taskkeeper",
  "version": "2.0.0",
  "description": "App to keep track of projects and invoices.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/braican/taskkeeper.git"
  },
  "author": "braican",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/braican/taskkeeper/issues"
  },
  "homepage": "https://github.com/braican/taskkeeper#readme",
  "dependencies": {
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
